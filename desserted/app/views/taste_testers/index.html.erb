<p id="notice"><%= notice %></p>

<h1>Taste Testers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Age</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @taste_testers.each do |taste_tester| %>
      <tr>
        <td><%= taste_tester.name %></td>
        <td><%= taste_tester.age %></td>
        <td><%= link_to 'Show', taste_tester %></td>
        <td><%= link_to 'Edit', edit_taste_tester_path(taste_tester) %></td>
        <td><%= link_to 'Destroy', taste_tester, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Taste Tester', new_taste_tester_path %>
